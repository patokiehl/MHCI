{
  "expo": {
    "name": "Todo",
    "slug": "Todo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
              }
    },
    "web": {
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(Todo) to use Face ID."
        }
      ],
      [
        "@react-native-voice/voice",{
          "microphonePermission": "Allow voice permission?",
          "speechRecognitionPermission": "Allow speech thingy"
        }
      ]
    ]
  }
}
